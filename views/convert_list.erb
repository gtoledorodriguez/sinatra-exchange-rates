<h1>Convert <%= @currency_symbol %></h1>
<p>Go <a href="/">back</a></p>
<ul>
  <% @currency_list.each do |currency_symbol| %>
    <li>
      <%= currency_symbol %>
      <a href="/<%= @currency_symbol %>/<%= currency_symbol %>">Convert 1 <%= @currency_symbol %> to <%= currency_symbol %>...</a>
    </li>
  <% end %>
</ul>
